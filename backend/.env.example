# ============================================
# RAG Chatbot - Environment Variables
# ============================================
# Copy this file to .env and fill in your values

# Required - Get from https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your_api_key_here

# Optional - Comma-separated CORS origins (default: http://localhost:5173)
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:5174

# Optional - Enable API key authentication (default: false)
REQUIRE_AUTH=false

# Optional - Comma-separated API keys for authentication (only if REQUIRE_AUTH=true)
API_KEYS=
